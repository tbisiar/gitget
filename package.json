{
    "name": "cc_img_handler",
    "version": "0.0.1",
    "description": "A social network for image resizing",
    "main": "index.ts",
    "author": "T Bisiar",
    "private": true,
    "license": "BSD-3-Clause",
    "scripts": {
        "start": "ts-node ./src/index.ts",
        "start:watch": "nodemon ./src/index.ts",
        "start:redis": "redis-server",
        "tsc": "tsc",
        "lint:fix": "eslint --fix",
        "test:lab": "lab --typescript -v ./test/endToEnd.test.ts",
        "test:lab:coverage": "lab --typescript -v ./test/endToEnd.test.ts -r html -o coverage.html",
        "test:unit": "npm run tsc && jest",
        "test": "npm run test:unit && npm run test:lab"
    },
    "dependencies": {
        "@hapi/catbox-memory": "^6.0.0",
        "@hapi/catbox-redis": "^7.0.0",
        "@hapi/hapi": "^20.2.2",
        "@hapi/joi": "^17.1.1",
        "dotenv": "^16.0.3",
        "redis-server": "^1.2.2"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@hapi/code": "^9.0.1",
        "@hapi/lab": "^25.0.1",
        "@types/hapi__catbox-memory": "^4.1.5",
        "@types/hapi__catbox-redis": "^5.0.5",
        "@types/hapi__hapi": "^20.0.13",
        "@types/jest": "^29.2.0",
        "@types/needle": "^2.5.3",
        "@types/node": "^18.11.5",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "eslint": "^8.26.0",
        "jest": "^29.2.2",
        "msw": "^0.48.0",
        "needle": "^3.1.0",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "./jest.setup.js"
        ],
        "testPathIgnorePatterns": [
            "test/endToEnd.test.ts"
        ],
        "coveragePathIgnorePatterns": [
            "test/endToEnd.test.ts"
        ]
    }
}